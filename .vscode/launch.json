  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "JS examples debug",
      "program": "${workspaceFolder}/examples/authorization/index.js",
      "skipFiles": ["<node_internals>/**/*.js"]
